{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "*": ["src/*"],
      "@/assets/*": ["src/assets/*"],
      "@/components/*": ["src/components/*"],
      "@/constants/*": ["src/constants/*"],
      "@/screens/*": ["src/screens/*"],
      "@/sheets/*": ["src/sheets/*"],
      "@/navigations/*": ["src/navigations/*"]
    }
  }
}
